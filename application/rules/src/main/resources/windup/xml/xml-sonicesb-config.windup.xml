<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:windup="http://www.jboss.org/schema/windup"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.jboss.org/schema/windup http://www.jboss.org/schema/windup.xsd">

    <windup:pipeline type="XML" id="xml-sonicesb-migration-decorators">

        <windup:xpath-classification description="SonicESB Service Configuration" xpath="/xq:serviceType"
            effort="4">
            <windup:namespace prefix="xq" uri="http://www.sonicsw.com/sonicxq"/>
        </windup:xpath-classification>

    </windup:pipeline>
</beans>
 